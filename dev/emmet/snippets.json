{
  "html": {
    "snippets": {
      "el-layout-hmf": "div[el=layout-hmf]>el-header+main[el=main]+footer[el=footer]",
      "el-layout-hm": "div[el=layout-hm]>el-header+main[el=main]",
      "el-layout-am": "div[el=layout-am]>aside[el=aside]+main[el=main]",
      "el-layout-asm": "div[el=layout-asm]>aside[el=aside]+section[el=section]+main[el=main]",
      "el-layout-ams": "div[el=layout-asm]>aside[el=aside]+main[el=main]+section[el=section]",
      "el-layout-aham": "div[el=layout-aham]>aside[el=aside]+el-header+main[el=main]",
      "el-layout-ahams": "div[el=layout-ahams]>aside[el=aside]+el-header+main[el=main]+section[el=section]",
